                 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee/web-app_2_5.xs"
                 version="2.5">
    <servlet>
        <servlet-name>Retriever</servlet-name>
        <servlet-class>com.sincerial.news.servlets.Retriever</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Retriever</servlet-name>
        <url-pattern>/feed</url-pattern>
    </servlet-mapping>
</web-app>